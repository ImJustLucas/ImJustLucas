{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "paths": {
      "@api/*": ["./src/api/*"],
      "@auth/*": ["./src/auth/*"],
      "@config/*": ["./src/config/*"],
      "@constants/*": ["./src/constants/*"],
      "@utils/*": ["./src/utils/*"],
      "@typesDef/*": ["./src/types/*"]
    },
    "types": ["node"],
    "jsx": "react-jsx"
  }
}